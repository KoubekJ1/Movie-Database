<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!--Server name-->
		<add key="DataSource" value="(localdb)\localDB1"/>
		
		<!--Database name-->
		<add key="InitialCatalog" value="DatabaseProject"/>
		
		<!--When using Windows authentication, set this to "true", otherwise set this to "false"-->
		<add key="IntegratedSecurity" value="true"/>
		
		<!--When using SQL server authentication, input your SQL server credentials-->
		<add key="UserID" value=""/>
		<add key="Password" value=""/>
		
		<!--Saved login credentials for a user inside the database-->
		<add key="Username" value=""/>
		<add key="User_Password" value=""/>
		
	</appSettings>
</configuration>